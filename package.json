{
	"name": "timestamp-helper",
	"displayName": "Timestamp Helper",
	"description": "easy to display and manipulate timestamp.",
	"publisher": "kovar",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.2"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kovarxu/vscode-timestamp-helper"
	},
	"activationEvents": [
		"onLanguage:plaintext",
		"onCommand:timestamp-helper.helloWorld",
		"onCommand:timestamp-helper.transfer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "timestamp-helper.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "timestamp-helper.transfer",
				"title": "transfer timestamp"
			}
		],
		"keybindings": [
			{
				"command": "timestamp-helper.transfer",
				"key": "alt+t alt+t",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.2",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"dayjs": "^1.9.6"
	}
}
